					while( !Obj_IsDeleted( enm_obj ) ){
						let rate = 12;
						ascent( idx in 0..length( slave ) ){
							if( Obj_IsDeleted( slave[ idx ] ) ){ rate += 22; }
						}
						ObjEnemy_SetDamageRate( enm_obj, rate, rate );
						life = ObjEnemy_GetInfo( enm_obj, INFO_LIFE );
