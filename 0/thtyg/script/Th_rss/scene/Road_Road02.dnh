function Road02(){
	let enm_num = GetEnemyIntersectionPosition(CenX, CenY, 512 );
	let plural;
	let midboss_flg = false;
	let compile_file_h	= [0,0];
	
	plural		= [csd ~ "./enm/Plural_stage02A.dnh",
					csd ~ "./enm/Plural_stage02B.dnh"];
	
	//　中ボスロード開始
	compile_file_h[0]	= LoadScriptInThread(plural[0]);
	LoadBossScript(compile_file_h[0]);
	
	
	
	
	//　nフレーム待機
	function FrameWait( n ){
		loop( n ){ yield };
	}
	//　自機ボム終了まで待機
	function WaitForSpell(){
		while( IsPlayerSpellActive() ){ yield; }
	}
	//　敵の数が 0 になるまで待機
	function WaitForZeroEnemy(){
		let AllEnemy	= GetAllEnemyID();
		let AllBoss		= GetEnemyBossObjectID();
		let AllScene	= GetEnemyBossSceneObjectID();
		while( length(AllEnemy) > 0 || length(enm_num) != 0 || length(AllBoss)!=0 || AllScene!=ID_INVALID ){
			AllEnemy	= GetAllEnemyID();
			AllBoss		= GetEnemyBossObjectID();
			AllScene	= GetEnemyBossSceneObjectID();
			yield;
		}
	}
	//　すべての敵を削除
	function VanishAllEnemy(){
		let AllEnemy	= GetAllEnemyID();
		let i			= 0;
		while(i<length(AllEnemy)){
			Obj_Delete(AllEnemy[i]);
			i++;
		}
	}
	
	//テスト用敵でーたー
	/*
	local{
		E_Pattern_Road02_6B( 20 );
		E_Pattern_Road02_6D( 20 );
		while( true ){ yield; }
	}
	//*/
	
	
	Stage02_WorldManager();
	
	FrameWait(60);
	
	//　引数2に情報セット　ｲﾍﾞﾝﾄ側でその情報を取り出す
	//　[ テクスチャ番号(0~2), 画像rect番号 ]
	NotifyEventAll( EV_USER + 2, [ 0, 3 ] );
	DeleteMusic();
	PlayMusic( 3, true, 0 );
	
	// 横切り妖精右
	task E_Pattern_Road02_3A( lp, by ){
		loop( lp ){
			CreateEnemy_Road02_3( [MaxX+64,by], [MinX-128,by], [512,128], 270, -3 );
			loop( 6 ){ yield; }
		}
	}
	// 横切り妖精左
	task E_Pattern_Road02_3B( lp, by ){
		loop( lp ){
			CreateEnemy_Road02_3( [MinX-64,by], [MaxX+128,by], [512,128], 270, 3 );
			loop( 6 ){ yield; }
		}
	}

	task E_Pattern_Road02_6A( lp ){
		loop( lp ){
			CreateEnemy_Road02_6( [MaxX+64,CenY-96], [MinX-128,CenY-64], [384,512], 90, 3 );
			loop( 6 ){ yield; }
		}
	}
	task E_Pattern_Road02_6B( lp ){
		loop( lp ){
			CreateEnemy_Road02_6( [MinX-64,CenY-96], [MaxX+128,CenY-64], [384,512], 90, -3 );
			loop( 6 ){ yield; }
		}
	}
	task E_Pattern_Road02_6C( lp ){
		loop( lp ){
			CreateEnemy_Road02_6( [MaxX+64,CenY-32], [MinX-64,CenY-96], [420,416], 120, 3 );
			loop( 6 ){ yield; }
		}
	}
	task E_Pattern_Road02_6D( lp ){
		loop( lp ){
			CreateEnemy_Road02_6( [MinX-64,CenY-32], [MaxX+64,CenY-96], [420,416], 60, -3 );
			loop( 6 ){ yield; }
		}
	}

	//　特攻ザコ妖精
	task E_Pattern_Road02_8A( lp, bx, wide ){
		loop( lp ){
			let pos = [ bx + wide * cos( frame * 8 ), MinY - 48 ];
//			let ag = atan2( ObjMove_GetY( GetPlayerObjectID ) - pos[ 1 ], ObjMove_GetX( GetPlayerObjectID ) - pos[ 0 ] );
			let ag = 90;
			
			CreateEnemy_Road02_8( pos, 2.5, ag );
			loop( 8 ){ yield; }
		}
	}
	
	let i = 0;
	
	//------------------Phase0---------------
	
	ascent( i in 0..15 ){
		CreateEnemy_Road02_7( [ MinX-64, MinY+128+128*cos(frame*13.7)  ], [ MaxX+256, CenY-64 ], [128,512], 270, 4 );
		CreateEnemy_Road02_7( [ MaxX+64, MinY+128+128*cos(frame*9.7) ], [ MinX-256, CenY-64 ], [128,512], 270, -4 );
		FrameWait(15);
	}
	ascent( i in 0..15 ){
		CreateEnemy_Road02_7( [ MinX-64, MinY+128+128*cos(frame*13.7) ], [ MaxX+256, CenY-64 ], [128,512], 90, 2 );
		CreateEnemy_Road02_7( [ MaxX+64, MinY+128+128*cos(frame*9.7) ], [ MinX-256, CenY-64 ], [128,512], 90, -2 );
		FrameWait(15);
	}
	
	StageTitleDisplay(180, csd ~ "./img/stg/stg2img/stg_t02a.png", [128,64,32] );
	FrameWait(300);
	
	//-----------------PhaseA-----------------
	E_Pattern_Road02_8A( 15, CenX + 96, 30 );
	
	loop( 1 ){
		ascent( i in 0..5 ){
			CreateEnemy_Road02_1( [ MinX+32 + i*32, MinY-64 ], [ MinX+32 + i*32, MaxY+256 ] );
			loop( 8 ){ yield; }
		}
	}
	if( IsOverDrive ){
		Set;
		task Set{
			ascent( i in 0..5 ){
				CreateEnemy_Road02_1( [ MaxX-32-i*32, MinY-64 ], [ MaxX-32-i*32, MaxY+256 ] );
				loop( 8 ){ yield; }
			}
		}
	}
	FrameWait( 70 );
	if( IsOverDrive ){
		Set;
		task Set{
			CreateEnemy_Road02_2( [ CenX-60,MinY+70 ], [ CenX, MinY - 256 ] );
			FrameWait( 50 );
			CreateEnemy_Road02_2( [ CenX-60,MinY+120 ], [ CenX, MinY - 256 ] );
			FrameWait( 50 );
			CreateEnemy_Road02_2( [ CenX-60,MinY+170 ], [ CenX, MinY - 256 ] );
		}
	}
	CreateEnemy_Road02_2( [ CenX+60,MinY+70 ], [ CenX, MinY - 256 ] );
	FrameWait( 50 );
	CreateEnemy_Road02_2( [ CenX+60,MinY+120 ], [ CenX, MinY - 256 ] );
	FrameWait( 50 );
	CreateEnemy_Road02_2( [ CenX+60,MinY+170 ], [ CenX, MinY - 256 ] );
	if( IsOverDrive ){
		E_Pattern_Road02_8A( 12, CenX - 96, 30 );
	}
	FrameWait( 80 );

	E_Pattern_Road02_3B( 10, CenY - 96 );
	FrameWait( 50 );
	E_Pattern_Road02_3B( 10, CenY - 36 );
	if( IsOverDrive ){
		E_Pattern_Road02_8A( 12, CenX + 96, 30 );
	}
	FrameWait( 90 );
	if( IsOverDrive ){
		CreateEnemy_Road02_2( [ CenX + 75, MinY + 150 ], [ CenX, MinY - 256 ] );
		CreateEnemy_Road02_2( [ CenX - 75, MinY + 150 ], [ CenX, MinY - 256 ] );
	}
	FrameWait( 90 );

	E_Pattern_Road02_8A( 24, CenX - 96, 30 );
	
	loop( 2 ){
		ascent( i in 0..5 ){
			CreateEnemy_Road02_1( [ MaxX-32 - i*32, MinY-64 ], [ MaxX-32 - i*32, MaxY+256 ] );
			loop( 8 ){ yield; }
		}
		FrameWait( 24 );
	}
	if( IsOverDrive ){
		Set;
		task Set{
			loop( 2 ){
				ascent( i in 0..5 ){
					CreateEnemy_Road02_1( [ MinX+32 + i*32, MinY-64 ], [ MinX+32 + i*32, MaxY+256 ] );
					loop( 8 ){ yield; }
				}
				FrameWait( 24 );
			}
		}
	}
	FrameWait( 90 );
	ascent( i in 0..5 ){
		CreateEnemy_Road02_2( [ CenX-70 + 48*i, MinY+70 + 30*i ], [ CenX, MinY - 256 ] );
		FrameWait( 20 );
		if( IsOverDrive ){
			CreateEnemy_Road02_2( [ CenX-94 + 48*i, MinY+86 + 30*i ], [ CenX, MinY - 256 ] );
		}
		FrameWait( 20 );
	}
	FrameWait( 90 );
	
	E_Pattern_Road02_3A( 12, CenY - 144 );
	FrameWait( 60 );
	E_Pattern_Road02_3A( 10, CenY - 80 );
	FrameWait( 50 );
	E_Pattern_Road02_3A( 8, CenY - 26 );
	FrameWait( 120 );
	loop( 1 ){
		ascent( i in 0..5 ){
			CreateEnemy_Road02_1( [ MinX+32 + i*32, MinY-64 ], [ MinX+32 + i*32, MaxY+256 ] );
			loop( 8 ){ yield; }
		}
		FrameWait( 50 );
		ascent( i in 0..5 ){
			CreateEnemy_Road02_1( [ MaxX-32 - i*32, MinY-64 ], [ MaxX-32 - i*32, MaxY+256 ] );
			loop( 8 ){ yield; }
		}
	}
	if( IsOverDrive ){
		Set;
		task Set{
			ascent( i in 0..5 ){
				CreateEnemy_Road02_1( [ MinX+32 + i*32, MinY-64 ], [ MinX+32 + i*32, MaxY+256 ] );
				loop( 8 ){ yield; }
			}
			FrameWait( 50 );
			ascent( i in 0..5 ){
				CreateEnemy_Road02_1( [ MaxX-32 - i*32, MinY-64 ], [ MaxX-32 - i*32, MaxY+256 ] );
				loop( 8 ){ yield; }
			}
		}
	}
	FrameWait( 90 );
	ascent( i in 0..5 ){
		CreateEnemy_Road02_2( [ CenX+70 - 48*i, MinY+70 + 30*i ], [ CenX, MinY - 256 ] );
		FrameWait( 10 );
		if( IsOverDrive ){
			CreateEnemy_Road02_2( [ CenX+94 - 48*i, MinY+86 + 30*i ], [ CenX, MinY - 256 ] );
		}
		FrameWait( 5 );
	}

	FrameWait( 90 );

	E_Pattern_Road02_8A( 15, CenX + 70, 30 );

	FrameWait( 100 );
	if( IsOverDrive ){
		CreateEnemy_Road02_2( [ CenX+50, MinY+130 ], [ CenX, MinY - 256 ] );
		CreateEnemy_Road02_2( [ CenX-50, MinY+130 ], [ CenX, MinY - 256 ] );
		CreateEnemy_Road02_2( [ CenX+90, MinY+100 ], [ CenX, MinY - 256 ] );
		CreateEnemy_Road02_2( [ CenX-90, MinY+100 ], [ CenX, MinY - 256 ] );
	}

	E_Pattern_Road02_8A( 15, CenX - 70, 30 );

	FrameWait( 90 );
	
	local{
		let lp = 2;
		let wt = 70;
		if( IsOverDrive ){
			lp = 3;
			wt = 47;
		}
		loop( lp ){
			ascent( i in 0..5 ){
				CreateEnemy_Road02_1( [ MaxX-32 - i*32, MinY-64 ], [ MaxX-32 - i*32, MaxY+256 ] );
			}
			FrameWait( wt );
			
			ascent( i in 0..5 ){
				CreateEnemy_Road02_1( [ MinX+32 + i*32, MinY-64 ], [ MinX+32 + i*32, MaxY+256 ] );
			}
			FrameWait( wt );
		}
	}

	E_Pattern_Road02_3A( 10, CenY - 100 );
	E_Pattern_Road02_3B( 10, CenY - 100 );
	
	FrameWait( 90 );

	E_Pattern_Road02_3A( 10, CenY - 166 );
	E_Pattern_Road02_3B( 10, CenY - 166 );
	
	FrameWait( 320 );
	//　中ボス＆早回し地帯
	
	WaitForZeroEnemy();
	VanishAllEnemy();
	WaitForSpell();
	DeleteShotAll(TYPE_ALL, TYPE_ITEM);
	FrameWait(60);
	midboss_flg = true;
	RegistBoss();
	FrameWait(120);
	IsBossDeleted;
	task IsBossDeleted{
		WaitForZeroEnemy();
		FrameWait( 120 );
		midboss_flg = false;
		//　次ボスロード開始
		compile_file_h[1]	= LoadScriptInThread(plural[1]);
		LoadBossScript(compile_file_h[1]);
	}
	FrameWait( 800 );
	//*/
	
	if( !midboss_flg ){
		Set;
		task Set{
			E_Pattern_Road02_3B( 10, CenY - 96 );
			FrameWait( 50 );
			E_Pattern_Road02_3B( 10, CenY - 36 );
			if( IsOverDrive ){
				FrameWait( 50 );
				CreateEnemy_Road02_5( [ CenX-64, MinY+72 ], [ CenX, MinY-256 ] );
				CreateEnemy_Road02_5( [ CenX-84, MinY+112 ], [ CenX, MinY-256 ] );
				CreateEnemy_Road02_5( [ CenX-44, MinY+112 ], [ CenX, MinY-256 ] );
			}
		}
	}
	FrameWait( 200 );
	if( !midboss_flg ){
		Set;
		task Set{
			E_Pattern_Road02_3A( 10, CenY - 96 );
			FrameWait( 50 );
			E_Pattern_Road02_3A( 10, CenY - 36 );
			if( IsOverDrive ){
				FrameWait( 50 );
				CreateEnemy_Road02_5( [ CenX+64, MinY+72 ], [ CenX, MinY-256 ] );
				CreateEnemy_Road02_5( [ CenX+84, MinY+112 ], [ CenX, MinY-256 ] );
				CreateEnemy_Road02_5( [ CenX+44, MinY+112 ], [ CenX, MinY-256 ] );
			}
		}
	}
	FrameWait( 200 );
	if( !midboss_flg ){
		E_Pattern_Road02_3A( 10, CenY - 100 );
		E_Pattern_Road02_3B( 10, CenY - 100 );
		if( IsOverDrive ){
			CreateEnemy_Road02_2( [ CenX+40,MinY+140 ], [ CenX, MinY - 256 ] );
			CreateEnemy_Road02_2( [ CenX-40,MinY+140 ], [ CenX, MinY - 256 ] );
		}
	}
	FrameWait( 100 );
	if( !midboss_flg ){
		E_Pattern_Road02_3A( 10, CenY - 166 );
		E_Pattern_Road02_3B( 10, CenY - 166 );
		if( IsOverDrive ){
			CreateEnemy_Road02_2( [ CenX+80,MinY+160 ], [ CenX, MinY - 256 ] );
			CreateEnemy_Road02_2( [ CenX-80,MinY+160 ], [ CenX, MinY - 256 ] );
		}
	}
	FrameWait( 200 );
	if( !midboss_flg ){
		Set;
		task Set{
			E_Pattern_Road02_3A( 10, CenY - 96 );
			FrameWait( 50 );
			E_Pattern_Road02_3A( 10, CenY - 36 );
			if( IsOverDrive ){
				FrameWait( 50 );
				let wide = 240;
				let lp = 8;
				let sx = CenX - wide / 2;
				loop( lp ){
					CreateEnemy_Road02_1( [ sx, MinY-64 ], [ sx, MaxY+256 ] );
					sx += wide / ( lp - 1 );
				}
			}
		}
	}
	FrameWait( 200 );
	if( !midboss_flg ){
		Set;
		task Set{
			E_Pattern_Road02_3B( 10, CenY - 96 );
			FrameWait( 50 );
			E_Pattern_Road02_3B( 10, CenY - 36 );
			if( IsOverDrive ){
				FrameWait( 50 );
				let wide = 320;
				let lp = 8;
				let sx = CenX - wide / 2;
				loop( lp ){
					CreateEnemy_Road02_1( [ sx, MinY-64 ], [ sx, MaxY+256 ] );
					sx += wide / ( lp - 1 );
				}
			}
		}
	}
	FrameWait( 100 );
	if( !midboss_flg ){
		Set;
		task Set{
			CreateEnemy_Road02_2( [ CenX+60,MinY+70 ], [ CenX, MinY - 256 ] );
			if( IsOverDrive ){
				CreateEnemy_Road02_5( [ CenX+90, MinY+95 ], [ CenX, MinY-256 ] );
			}
			FrameWait( 50 );
			CreateEnemy_Road02_2( [ CenX+60,MinY+120 ], [ CenX, MinY - 256 ] );
			if( IsOverDrive ){
				CreateEnemy_Road02_5( [ CenX+90, MinY+145 ], [ CenX, MinY-256 ] );
			}
			FrameWait( 50 );
			CreateEnemy_Road02_2( [ CenX+60,MinY+170 ], [ CenX, MinY - 256 ] );
			if( IsOverDrive ){
				CreateEnemy_Road02_5( [ CenX+90, MinY+195 ], [ CenX, MinY-256 ] );
			}
		}
	}
	FrameWait( 200 );
	if( !midboss_flg ){
		Set;
		task Set{
			CreateEnemy_Road02_2( [ CenX-60,MinY+70 ], [ CenX, MinY - 256 ] );
			if( IsOverDrive ){
				CreateEnemy_Road02_5( [ CenX-90, MinY+95 ], [ CenX, MinY-256 ] );
			}
			FrameWait( 50 );
			CreateEnemy_Road02_2( [ CenX-60,MinY+120 ], [ CenX, MinY - 256 ] );
			if( IsOverDrive ){
				CreateEnemy_Road02_5( [ CenX-90, MinY+145 ], [ CenX, MinY-256 ] );
			}
			FrameWait( 50 );
			CreateEnemy_Road02_2( [ CenX-60,MinY+170 ], [ CenX, MinY - 256 ] );
			if( IsOverDrive ){
				CreateEnemy_Road02_5( [ CenX-90, MinY+195 ], [ CenX, MinY-256 ] );
			}
		}
	}
	FrameWait( 200 );
	if( !midboss_flg ){
		Set;
		task Set{
			E_Pattern_Road02_8A( 15, CenX + 70, 30 );
			FrameWait( 100 );
			E_Pattern_Road02_8A( 15, CenX - 70, 30 );
		}
	}
	FrameWait( 200 );
	if( !midboss_flg ){
