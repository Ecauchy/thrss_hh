						let sx = ObjMove_GetX( master ) + vx[ idx ];
						let sy = ObjMove_GetY( master ) + vy[ idx ];
						ObjMove_SetPosition( obj, sx, sy );
						if( Obj_IsDeleted( master ) ){
							if( life > 0 ){ Obj_Delete( obj ); return; }
							break;
						}
						yield;
