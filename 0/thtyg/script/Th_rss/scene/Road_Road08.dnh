	task E_Pattern_Road08_15A( bx, by, type ){
		let vx = 36;
		let vy = 36;
		let sx = [ bx, bx - vx, bx - vx, bx - vx, bx, bx + vx, bx + vx, bx + vx ];
		let sy = [ by - vy, by - vy, by, by + vy, by + vy, by + vy, by, by - vy ];
		let ex = [ bx, bx + vx*2, bx + vx*2, bx + vx*2, bx, bx - vx*2, bx - vx*2, bx - vx*2 ];
		ascent( idx in 0..length( sx ) ){
			let obj = CreateEnemy_Road08_15( [ bx, MinY - 64 ], [ sx[ idx ], sy[ idx ] ], [ sx[ idx ], MaxY + 255 ] );
			if( type == 1 ){ ar_enm = ar_enm~[ obj ]; }
			if( type == 2 ){ ar_enmB = ar_enmB~[ obj ]; }
		}
	}

	task E_Pattern_Road08_17A( lp, bx, wide, type ){
		loop( lp ){
			let pos = [ bx + wide * cos( frame * 13 ), MinY - 48 ];
			let ag = atan2( ( MinY - 255 ) - pos[ 1 ], CenX - pos[ 0 ] ) + 180;
			let speed = 3 + 0.5 * cos( frame * 9 );
			let obj = CreateEnemy_Road08_17( pos, speed, ag );
			if( type == 1 ){ ar_enm = ar_enm~[ obj ]; }
			if( type == 2 ){ ar_enmB = ar_enmB~[ obj ]; }
			loop( 5 ){ yield; }
		}
	}
	
