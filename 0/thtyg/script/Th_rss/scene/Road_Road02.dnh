		task Set{
			let lp = 5;
			let wt = 60;
			if( IsOverDrive ){ lp = 10; wt = 8; }
			ascent( i in 0..lp ){
				CreateEnemy_Road02_1( [ MinX+32 + i*32, MinY-64 ], [ MinX+32 + i*32, MaxY+256 ] );
				loop( 8 ){ yield; }
			}
			FrameWait( wt );
			ascent( i in 0..lp ){
				CreateEnemy_Road02_1( [ MaxX-32 - i*32, MinY-64 ], [ MaxX-32 - i*32, MaxY+256 ] );
				loop( 8 ){ yield; }
			}
		}
	}
	FrameWait( 200 );
	if( !midboss_flg ){
		Set;
		task Set{
			let lp = 5;
			let wt = 60;
			if( IsOverDrive ){ lp = 10; wt = 8; }
			ascent( i in 0..lp ){
				CreateEnemy_Road02_1( [ MinX+32 + i*32, MinY-64 ], [ MinX+32 + i*32, MaxY+256 ] );
				loop( 8 ){ yield; }
			}
			FrameWait( wt );
			ascent( i in 0..lp ){
				CreateEnemy_Road02_1( [ MaxX-32 - i*32, MinY-64 ], [ MaxX-32 - i*32, MaxY+256 ] );
				loop( 8 ){ yield; }
			}
		}
	}
	FrameWait( 200 );
	
	if( !midboss_flg ){
		E_Pattern_Road02_3A( 10, CenY - 166 );
		E_Pattern_Road02_3B( 10, CenY - 166 );
